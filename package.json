{
  "name": "discordjs-bot-standard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/app/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node --experimental-specifier-resolution=node dist/app/index.js",
    "register:guild": "tsx scripts/deploy-commands.ts --guild",
    "register:global": "tsx scripts/deploy-commands.ts --global",
    "prune": "tsx src/scripts/prune-commands.ts",
    "lint": "echo \"(optional) add eslint here\""
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "chalk": "^5.5.0",
    "discord.js": "^14.21.0",
    "dotenv": "^16.6.1",
    "fast-glob": "^3.3.2",
    "figlet": "^1.8.2",
    "moment-timezone": "^0.6.0",
    "node-cron": "^3.0.3",
    "pino": "^9.4.0",
    "timezone": "^1.0.23",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^22.18.0",
    "@types/node-cron": "^3.0.11",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
